{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": [
      "app/common/js",
      "app/web/private/backend/js",
      "app/web/private/frontend/js"
    ],
    "includePattern": ".+\\.js(doc)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "opts": {
    "template": "./node_modules/ink-docstrap/template",
    "encoding": "utf8",
    "destination": "./docs/",
    "recurse": true
  },
  "plugins": [],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": true
    },
    "systemName": "Brainch",
    "footer": "",
    "copyright": "Brainch Â© 2015",
    "navType": "vertical",
    "theme": "united",
    "linenums": true,
    "collapseSymbols": false,
    "inverseNav": true
  }
}