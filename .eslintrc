{
  "ecmaFeatures": {
    "jsx": false,
    "modules": true
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": false
  },
  "rules": {
    "quotes": [2, "single"],
    "eol-last": [0],
    "no-mixed-requires": [0],
    "no-underscore-dangle": [0],
    "no-unused-vars": [2, {"vars": "all", "args": "none"}],
    "new-cap": [2, { "capIsNewExceptions": ["List", "Map"] }],
    "no-extend-native": [0],
    "camelcase": [0],
    "no-shadow": [0],
    "no-use-before-define": [0, "nofunc"]
  }
}
